def main():
    print("Шановний користувач! Дана програма створена для того, аби Ви дізналися відповіді на питання, які Вас цікавлять.")
print("Оберіть одну з наступних сфер:")
print("1. Кохання/відносини")
print("2. Кар'єра")
print("3. Опис/характеристика людини")
print("4. Карта дня")
print("5. Фінанси")
print("6. Друзі")
print("7. Життя")
print("8. Сім'я")
print("9. Здоров'я")

selectedOption = input()

if selectedOption.isdigit():
    option = int(selectedOption)
    if option == 1:
            selectedFormat = int(input("Оберіть формат відповіді:\n1. Дата\n2. Порада\n3. \"Так\" чи \"Ні\"\n4. Про майбутнє\n5. Місце\n"))
            if 1 <= selectedFormat <= 6:
                if selectedFormat == 1:
                    question1 = input("Введіть своє питання: ")
                    print(date(get_random_card()))
                elif selectedFormat == 2:
                    print(advice_love(get_random_card()))
                elif selectedFormat == 3:
                    question2 = input("Введіть своє питання: ")
                    print(yes_or_no(get_random_card()))
                elif selectedFormat == 4:
                    print(future_love(get_random_card()))
                elif selectedFormat == 5:
                    print(feeling(get_random_card()))
                elif selectedFormat == 6:
                    question3 = input("Введіть своє питання: ")
                    print(place(get_random_card()))
    elif option == 2:
            selectedFormat = int(input("Оберіть формат відповіді:\n1. Дата\n2. Порада\n3. \"Так\" чи \"Ні\"\n4. Про майбутнє\n5. Місце\n"))
