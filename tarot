                elif selectedFormat == 5:
                    question3 = input("Введіть своє питання: ")
                    print(place(get_random_card()))
    elif option == 7:
            selectedFormat = int(input("Оберіть формат відповіді:\n1. Дата\n2. Порада\n3. \"Так\" чи \"Ні\"\n4. Про майбутнє\n5. Місце\n"))
            if 1 <= selectedFormat <= 5:
                if selectedFormat == 1:
                    question1 = input("Введіть своє питання: ")
                    print(date(get_random_card()))
                elif selectedFormat == 2:
                    print(advice_family(get_random_card()))
                elif selectedFormat == 3:
                    question2 = input("Введіть своє питання: ")
                    print(yes_or_no(get_random_card()))
                elif selectedFormat == 4:
                    print(future_family(get_random_card()))
                elif selectedFormat == 5:
                    question3 = input("Введіть своє питання: ")
                    print(place(get_random_card()))
    elif option == 8:
            selectedFormat = int(input("Оберіть формат відповіді:\n1. Дата\n2. Порада\n3. \"Так\" чи \"Ні\"\n4. Про майбутнє\n5. Місце\n"))
            if 1 <= selectedFormat <= 5:
                if selectedFormat == 1:
                    question1 = input("Введіть своє питання: ")
                    print(date(get_random_card()))
                elif selectedFormat == 2:
                    print(advice_life(get_random_card()))
                elif selectedFormat == 3:
                    question2 = input("Введіть своє питання: ")
                    print(yes_or_no(get_random_card()))
                elif selectedFormat == 4:
                    print(future_life(get_random_card()))
                elif selectedFormat == 5:
                    question3 = input("Введіть своє питання: ")
                    print(place(get_random_card()))
    elif option == 9:
            selectedFormat = int(input("Оберіть формат відповіді:\n1. Дата\n2. Порада\n3. \"Так\" чи \"Ні\"\n4. Про майбутнє\n5. Місце\n"))
            if 1 <= selectedFormat <= 4:
                if selectedFormat == 1:
                    question1 = input("Введіть своє питання: ")
                    print(date(get_random_card()))
                elif selectedFormat == 2:
                    print(advice_health(get_random_card()))
                elif selectedFormat == 3:
                    question2 = input("Введіть своє питання: ")
                    print(yes_or_no(get_random_card()))
                elif selectedFormat == 4:
                    print(future_health(get_random_card()))
    else:
        print("Будь ласка, введіть коректний номер від 1 до 9.")
else:
    print("Будь ласка, введіть коректний номер від 1 до 9.")

if __name__ == "__main__":
    main()
    
